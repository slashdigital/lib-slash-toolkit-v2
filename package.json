{
  "name": "lib-slash-toolkit-v2",
  "version": "2.0.1",
  "description": "Lib Slash Toolkit for Rapid Prototyping",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.(js|ts)\"",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint src --fix --ext .js,.ts",
    "test": "jest --config jest.config.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/slash-venture-pte/lib-slash-toolkit-v2.git"
  },
  "keywords": [
    "slash",
    "rapid",
    "development"
  ],
  "author": "Slash",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/slash-venture-pte/lib-slash-toolkit-v2/issues"
  },
  "devDependencies": {
    "@types/jest": "28.1.6",
    "@typescript-eslint/eslint-plugin": "5.30.7",
    "@typescript-eslint/parser": "5.30.7",
    "eslint": "8.20.0",
    "eslint-plugin-jest": "26.6.0",
    "jest": "28.1.3",
    "prettier": "2.7.1",
    "ts-jest": "^28.0.8",
    "typescript": "4.7.4"
  },
  "files": [
    "lib/**/*"
  ],
  "homepage": "https://github.com/slash-venture-pte/lib-slash-toolkit-v2#readme"
}
